{
  "extends": "airbnb-base",
  "rules": {
    "strict": 0,
    "one-var": 0,
    "prefer-const": 0,
    "prefer-destructuring": 0,
    "one-var-declaration-per-line": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "no-bitwise": 0,
    "class-methods-use-this": 0,
    "no-sparse-arrays": 0,
      "quotes": [
        1,
      "single"
      ]
  },
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
}
